<!doctype html>
<html>
    <head>
        <meta charset="utf-8">
        <title>Bosetterne - Registrering</title>
        <link rel="stylesheet" href="../ressurser/css/sections.css">
        <link rel="stylesheet" href="../ressurser/css/tables.css">
        <link rel="stylesheet" href="../ressurser/css/text.css">
        <link rel="stylesheet" href="../ressurser/css/forms.css">
    </head>
    <body>
        <header>
            <hgroup>
                <h1>Spill</h1>
            </hgroup>
        </header>
        <nav>
            <p class="meny"><span>Meny</span><p>
            <ul class="menyliste">
                <li class="meny"><a class="meny" href="hjem">Hjem</a></li>
                <li class="meny"><a class="meny" href="registrer">Registrer</a></li>
                <li class="meny"><a class="meny" href="logg_inn">Logg inn</a></li>
                {{#loggedIn}}
                    <li class="meny" v><a class="meny" href="bosetterne">Bosetterne</a></li>
                    <li class="meny"><a class="meny" href="../ressurser/chat.html">Chat</a></li>
                {{/loggedIn}}
            </ul>
        </nav>
        <article>
            <h1>Registrer deg for tilgang</h1>
            
            <form id="frmRegister"
                  enctype="application/x-www-form-urlencoded"
                  accept-charset="utf-8"
                  action="">
                  <label for="txtKalleNavn">Kallenavn:</label>
                  <input type="text" id="txtKalleNavn" name="txtKallenavn" class="felter" value="" size="15" maxlength="15">
                  <br/>
                  
                  <label class="farge" for="clrFarge">Ønsket farge:</label>
                  <input type="color" id="clrFarge" name="clrFarge" class="felter" value="">
                  <br/>
                  
                  <label for="txtEpost">Epost:</label>
                  <input type="email" id="txtEpost" name="txtEpost" class="felter" value="" size="25" maxlength="25">
                  <br/>
                  
                  <label for="pass1">Passord:</label>
                  <input type="password" id="pass1" name="pass1" class="felter" value="" size="15" maxlength="15">
                  <br/>
                  
                  <label for="pass2">Passord (igjen):</label>
                  <input type="password" id="pass2" name="pass2" class="felter" value="" size="15" maxlength="15">
                  <br/>
                  
                  <input type="submit" value="Send inn">
            </form>

        </article>
        <footer>
            All rights reserved © Espen
        </footer>
    </body>
</html>

